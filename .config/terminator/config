  enabled_plugins = ActivityWatch, TerminalShot, LaunchpadCodeURLHandler, APTURLHandler, LaunchpadBugURLHandler
[keybindings]
  next_tab = <Alt>Right
  go_up = <Shift><Control>Up
  resize_up = None
  resize_left = None
  prev_tab = <Alt>Left
  go_down = <Shift><Control>Down
  go_right = <Shift><Control>Right
  go_left = <Shift><Control>Left
  resize_right = None
  resize_down = None
[profiles]
