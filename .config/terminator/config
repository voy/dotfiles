  enabled_plugins = ActivityWatch, TerminalShot, LaunchpadCodeURLHandler, APTURLHandler, LaunchpadBugURLHandler
[keybindings]
  next_tab = <Alt>Right
  go_up = <Shift><Control>Up
  resize_up = <Shift><Control><Alt>Up
  resize_left = <Shift><Control><Alt>Left
  prev_tab = <Alt>Left
  go_down = <Shift><Control>Down
  go_right = <Shift><Control>Right
  go_left = <Shift><Control>Left
  resize_right = <Shift><Control><Alt>Right
  resize_down = <Shift><Control><Alt>Down
[profiles]
  [[default]]
    scrollbar_position = hidden
    scroll_on_output = False
    font = Monospace 11
    use_system_font = False
    scrollback_infinite = True
[layouts]
  [[default]]
    [[[child1]]]
      type = Terminal
      parent = window0
    [[[window0]]]
      type = Window
      parent = ""
[plugins]
